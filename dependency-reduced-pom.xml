  <developers>
    <developer>
      <id>febiAlfarabi</id>
      <name>Alfarabi Dwi Karuiawan</name>
      <email>alfarabidwik@gmail.com</email>
      <organization>Numeral Asia</organization>
      <organizationUrl>https://numeralasia.com/</organizationUrl>
      <roles>
        <role>architect</role>
        <role>developer</role>
        <role>founder</role>
      </roles>
